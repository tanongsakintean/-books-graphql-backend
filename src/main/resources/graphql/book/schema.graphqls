type Book {
    bookId: ID
    title: String
    isbn: String
    publicDate: String
    publisherId: Int
    genreId: Int
    summary: String
    pageCount: Int
}

type BookDetail {
    book: Book
    publisher: Publisher
    genre: Genre
}

type BookAuthor {
    author:Author
    books:[BookDetail]
}


input BookInput {
    title: String
    isbn: String
    publicDate: String
    publisherId: Int
    genreId: Int
    summary: String
    pageCount: Int
    authorId: [Int]
}
